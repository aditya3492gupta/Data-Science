    "def linearsearch(array,n,k):\n",
    "    for j in range(0,n):\n",
    "        if(array[j]==k):\n",
    "            return j\n",
    "    return -1\n",
    "\n",
    "array=[1,3,5,7,9]\n",
    "\n",
    "k=7\n",
    "n=len(array)\n",
    "result=linearsearch(array,n,k)\n",
    "if(result==-1):\n",
    "    print(\"Element not found\")\n",
    "else:\n",
    "    print(\"Element found at index \",result)"
   ]
